import turtle
turtle = turtle.Turtle()
turtle.position()

turtle.home()
turtle.clear()
turtle.forward(200)
turtle.left(120)
turtle.forward(200)
turtle.left(120)
turtle.forward(200)

turtle.home()
turtle.clear()
for i in range(3):
turtle.forward(200)
turtle.left(120)

turtle.home()
turtle.clear()
for i in range(5):
turtle.forward(200)
turtle.left(144)

t = turtle

t.home()
t.clear()

for i in range(4):
 t.left(90)
 